  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20230307.0",
    "@remix-run/dev": "^2",
    "@remix-run/server-runtime": "^2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
